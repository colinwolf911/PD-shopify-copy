    vertical-align: middle;
  }

  .cart-qty {margin-right: 3px;}
}

.sticky-header {
  .header-wrapper {
    .cart-design-1 {
      padding: 0;

      .icon-cart-header {
        .cart-total {
          display: block;
          position: absolute;
          top: 50%;
          left: 0;
          width: 100%;
