            cursor: pointer;
            font-family: 'Open Sans';

            span.name {line-height: 32px;}

            &.disable {
              display: none !important;
              cursor: not-allowed;

              .has-image {
                &:after {
                  display: block;
                  position: absolute;
                  top: 10px;
                  right: -4px;
                  width: 30px;
                  height: 2px;
                  transform: rotate(-45deg);
                  -ms-transform: rotate(-45deg);
                  -webkit-transform: rotate(-45deg);
                  background-color: #E0E0E0;
                  content: ' ';
                }
              }

              &:hover {
                .has-image {border: #e0e0e0 solid 1px;}
              }

              .input-skin {
                &:after {
                  display: block;
                  position: absolute;
                  top: 8.4px;
                  right: 5px;
                  width: 11px;
                  height: 2px;
                  -ms-transform: rotate(-45deg);
                  -webkit-transform: rotate(-45deg);
                  transform: rotate(-45deg);
                  background-color: #efefef;
                  content: ' ';
