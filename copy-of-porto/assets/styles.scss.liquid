.mobile-canvas-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  transition: all .3s;
  background-color: #000;
  opacity: 0;
  visibility: hidden;
  z-index: 100;
  filter: alpha(opacity = 0);
}

.mobile-canvas-shown {
  .block-main-canvas {
    right: 0;

    .canvas-inner {
      display: block;
      height: 100%;
      padding-right: 10px;
      padding-left: 10px;
      overflow-x: hidden;
      overflow-y: auto;
    }

    .canvas-icon {
      opacity: 1;
      visibility: visible;

      i::before {
        font-family: Porto;
        content: '\e82c';
      }
    }
  }

  .mobile-canvas-overlay {
    opacity: .5;
    visibility: visible;
    filter: alpha(opacity = 15);
  }

  .col-left .block-main-canvas {
    right: auto;
    left: 0;
  }

  .col-right .block-main-canvas {
    right: 0;
    left: auto;
  }

  .sidebar .block::before {display: none;}
}

.block-main-canvas {
  .canvas-icon {
    position: absolute;
    top: 22%;
    right: -40px;
    width: 40px;
    height: 40px;
