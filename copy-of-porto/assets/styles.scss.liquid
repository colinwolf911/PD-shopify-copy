// MAIN CONTAINER STYLE
.main-container {
  h3.title-widget {
    margin: 20px 0 0;
    background: url(//cdn.shopify.com/s/files/1/1613/0131/t/6/assets/slider-bar.png?9083702354724775921) right no-repeat;
    color: #313131;
    font-size: 16px;
    font-weight: 700;
    line-height: 42px;
    text-align: left;
    text-transform: uppercase;

    span.inner {
      padding-right: 20px;
      background-color: #fff;
    }
  }

  .products-collection {
    position: relative;

    h3.title-widget {
      position: relative;

      &.style_1 {
        margin: 30px 0 0;
        background: url(slider-bar.png) right no-repeat;
        color: #313131;
        font-size: 16px;
        font-weight: 700;
        line-height: 42px;
        text-align: left;
        text-transform: uppercase;

        span.inner {
          padding-right: 20px;
          background-color: #fff;
        }
      }

      &.style_2 {
        margin-top: 26px;
        margin-bottom: 15px;
        background: none;
        font-size: 19px;
        font-weight: 600;
        text-align: center;
      }

      &.style_3 {
        background: none;
        text-align: center;

        span.inner {
          padding-left: 20px;
          background-color: #fff;
        }

        &::after {
          position: absolute;
          top: 50%;
          left: 0;
          width: 100%;
          height: 1px;
          background-color: #ccc;
          content: '';
          z-index: -1;
        }
      }

      &.style_4 {
        margin-top: 24px;
        margin-bottom: 17px;
        border-bottom: #e1e1e1 solid 1px;
        background: none;
        font-size: 18px;
        font-weight: 700;
        line-height: 42px;
        text-transform: uppercase;
      }
    }

    .category-products {
      .products-grid {
        &.center_nav {
          .owl-nav {
            position: static;

            button {
              position: absolute;
              top: 50%;
              -webkit-transform: translateY(-50%);
              transform: translateY(-50%);
              font-size: 24px;
            }

            .owl-prev {left: -40px;}

            .owl-next {right: -40px;}

            .item {
              width: 100%;
              padding: 10px 0;
            }
          }
        }

        &.top_paginate {
          .owl-dots {
            position: absolute;
            top: -52px;
            right: 10px;
            bottom: auto;
            left: inherit;
            margin: 0;
          }
        }
      }

      .owl-nav {
        position: absolute;
        top: -30px;
        right: 5px;

