    padding: 16px 0;
    background-color: #171717;
    color: #fff;
    font-size: 13px;
    line-height: 1;
  }

  .breadcrumbs {
    ul {
      margin: 0;
      overflow: hidden;

      li {
        display: inline-block;

        a {
